<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin.calandburn.SearchNameFood"
    android:background="@drawable/bg4">

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#000000" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:id="@+id/imageView17"
        android:onClick="backToMainMenu"
        android:background="@drawable/banner2" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView6"
        android:layout_below="@+id/editText11"
        android:layout_margin="5dp" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/search_namefood"
        android:id="@+id/editText11"
        android:layout_below="@+id/imageView16"
        android:layout_margin="20dp" />

    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/toReport"
        android:layout_alignParentRight="true"
        android:background="@drawable/reporttop"
        android:onClick="clickToReportFromSearchFood" />

</RelativeLayout>
